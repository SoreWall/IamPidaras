<Page x:Class="coursework01.Pages.ProfilePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:coursework01.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ProfilePage">
    <Grid Background="AliceBlue">
        <StackPanel>
            <Button Name="ExitButton" Content="Выйти из профиля" Style="{StaticResource RegisterButton}" 
                    Click="ExitButton_Click" Width="243" HorizontalAlignment="Left"/>
            <TextBlock Name="TB" FontSize="15"></TextBlock>
        </StackPanel>

        <StackPanel VerticalAlignment="Bottom">
            <Button Name="CatalogButton" Content="Каталог авто" Style="{StaticResource RegisterButton}" Click="CatalogButton_Click"/>
        </StackPanel>
    </Grid>
</Page>
